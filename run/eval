#! /usr/bin/env node

require('console-ultimate/global').replace();

var load = JSON.parse;

var inputOrFile = require('../src/cli-util/input-or-file');

var Frame = require('../src/runtime/Frame');

inputOrFile(process.argv[2], function (ast)
{
	ast = load(ast);

	var frame = new Frame(ast);
	frame.run();
});
